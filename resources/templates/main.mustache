{{> header}}

<script src="js/visual.js" type="text/javascript"></script>
<script>
function init() {
    var canvas=document.getElementById("mainBox");
    var context=canvas.getContext("2d");

    var x=10;
    var y=canvas.height/2;
    context.font = 'italic 80px Calibri';
    context.textBaseline = 'middle';
    context.fillText("Digital Experiences", x, y);
}

</script>

<div class="container">
    <div class="row">
        <div class="col-sm-12">
            <canvas id="mainBox" width="800" height="200"></canvas>
        </div> <!-- /column -->
    </div> <!-- /row -->
</div>

<script>
init();
</script>

{{> footer}}
